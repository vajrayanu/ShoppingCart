                                if (Companies.ContactDistrictID == it.DistrictID)
                                    {
                                        <option value="@it.DistrictID" selected="selected">@it.DistrictName</option>
                                    }
                                else
                                    {
                                        <option value="@it.DistrictID">@it.DistrictName</option>
                                    }
                            }
                        </select>
                    </div>
                </div>  
